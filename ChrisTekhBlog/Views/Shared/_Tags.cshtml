@model IList<ChrisTekhBlog.Core.Objects.Tag>

<div >
  

    @if (Model.Count > 0)
    {
        foreach (var tag in Model)
        {
            <a href="~/Tag/@tag.UrlSlug">@tag.Name</a><text>, &nbsp;</text>
        }
    }
    else
    {
        <p>No tags found!</p>
    }
</div>
